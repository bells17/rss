<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<h2>Feeds</h2>

<div>
  <% @data[:feeds].each do |feed| %>
    <h3>Title: <%= feed.title %></h3>
    <h3>URL: <%= feed.url %></h3>
    <ul>
      <% feed.entries.each do |entry| %>
        <li>
          <a href="<%= entry.url %>"><%= entry.title %></a> - <span><%= entry.summary %></span>
          <img src="https://farm8.staticflickr.com/7545/15973514551_eafdeef504_c.jpg">
        </li>
      <% end %>
    </ul>
  <% end %>
</div>
